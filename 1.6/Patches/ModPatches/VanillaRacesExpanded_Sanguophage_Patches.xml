<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
		  <li>Vanilla Races Expanded - Sanguophage</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/GeneDef[defName="VRE_Sensitivity_Dangerous"]/suppressedTraits</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>/Defs/GeneDef[defName="VRE_Sensitivity_Dangerous"]</xpath>
						<value>
							<suppressedTraits>
								<li><def>HVT_Sunbather</def></li>
							</suppressedTraits>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>/Defs/GeneDef[defName="VRE_Sensitivity_Dangerous"]/suppressedTraits</xpath>
						<value>
							<li><def>HVT_Sunbather</def></li>
						</value>
					</match>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>