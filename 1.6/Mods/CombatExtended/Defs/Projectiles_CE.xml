<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!--bluejay-->
  <ThingDef ParentName="BaseBulletCE" MayRequire="Ludeon.RimWorld.Royalty">
    <defName>PsychicBluejayProjectile_CE</defName>
    <label>psychic attack rush</label>
    <graphicData>
      <texPath>Things/Ethereal/Tornado</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
	<projectile Class="CombatExtended.ProjectilePropertiesCE">
		<damageDef>Hauts_PsychicAttackRush</damageDef>
		<speed>1000</speed>
		<damageAmountBase>9</damageAmountBase>
		<armorPenetrationSharp>6.5</armorPenetrationSharp>
		<armorPenetrationBlunt>39.36</armorPenetrationBlunt>
	</projectile>
  </ThingDef>
  
  <!--fulmar-->
  <ThingDef ParentName="BaseBullet">
    <defName>HVT_PsychicFulmarProjectile_CE</defName>
    <label>psychic siege blast</label>
	<graphicData>
	  <graphicClass>Graphic_Single</graphicClass>
	  <texPath>Things/Mote/PsycastCast</texPath>
	  <shaderType>MoteGlowDistorted</shaderType>
	  <color>(55,55,185)</color>
	  <drawSize>3</drawSize>
	  <shaderParameters>
		<_DistortionTex>/Other/Ripples</_DistortionTex>
		<_distortionIntensity>0.11</_distortionIntensity>
		<_DistortionScale>0.4</_DistortionScale>
		<_distortionScrollSpeed>(0.5, 0.5, 0.0)</_distortionScrollSpeed>
	  </shaderParameters>
	</graphicData>
    <thingClass>Projectile_Explosive</thingClass>
    <projectile>
      <damageDef>Hauts_PsychicBlast</damageDef>
      <!-- damage amount is the damage def default -->
      <speed>41</speed>
      <explosionRadius>2.9</explosionRadius>
	  <damageAmountBase>150</damageAmountBase>
	  <stoppingPower>0.5</stoppingPower>
	  <flyOverhead>true</flyOverhead>
      <soundHitThickRoof>Artillery_HitThickRoof</soundHitThickRoof>
      <soundExplode>MortarBomb_Explode</soundExplode>
      <soundImpactAnticipate>MortarRound_PreImpact</soundImpactAnticipate>
      <soundAmbient>MortarRound_Ambient</soundAmbient>
      <preExplosionSpawnSingleThingDef>Filth_BlastMark</preExplosionSpawnSingleThingDef>
    </projectile>
  </ThingDef>
  
  <!--hornet-->
  <ThingDef ParentName="BaseBulletCE" MayRequire="Ludeon.RimWorld.Royalty">
    <defName>HVT_PsychicHornetProjectile_CE</defName>
    <label>psychic blast projectile</label>
	<graphicData>
	  <graphicClass>Graphic_Single</graphicClass>
	  <texPath>Things/Projectile/ChargeLanceShot</texPath>
	  <shaderType>MoteGlowDistorted</shaderType>
	  <color>(255,10,255)</color>
	  <drawSize>1</drawSize>
	  <shaderParameters>
		<_DistortionTex>/Other/Ripples</_DistortionTex>
		<_distortionIntensity>0.11</_distortionIntensity>
		<_DistortionScale>0.4</_DistortionScale>
		<_distortionScrollSpeed>(0.5, 0.5, 0.0)</_distortionScrollSpeed>
	  </shaderParameters>
	</graphicData>
	<projectile Class="CombatExtended.ProjectilePropertiesCE">
		<damageDef>Hauts_PsychicBlast</damageDef>
		<speed>120</speed>
		<damageAmountBase>4</damageAmountBase>
		<dropsCasings>false</dropsCasings>
		<explosionRadius>1.9</explosionRadius>
		<explosionDelay>10</explosionDelay>
		<armorPenetrationSharp>6.5</armorPenetrationSharp>
		<armorPenetrationBlunt>39.36</armorPenetrationBlunt>
	</projectile>
  </ThingDef>
  
  <ThingDef ParentName="BaseBulletCE" MayRequire="Ludeon.RimWorld.Royalty">
	<defName>HVT_PsychicYellowjacketProjectile_CE</defName>
    <label>psychic bolt projectile</label>
	<graphicData>
	  <graphicClass>Graphic_Single</graphicClass>
	  <texPath>Things/Projectile/ChargeLanceShot</texPath>
	  <shaderType>MoteGlowDistorted</shaderType>
	  <color>(255,255,10)</color>
	  <drawSize>1</drawSize>
	  <shaderParameters>
		<_DistortionTex>/Other/Ripples</_DistortionTex>
		<_distortionIntensity>0.11</_distortionIntensity>
		<_DistortionScale>0.4</_DistortionScale>
		<_distortionScrollSpeed>(0.5, 0.5, 0.0)</_distortionScrollSpeed>
	  </shaderParameters>
	</graphicData>
	<projectile Class="CombatExtended.ProjectilePropertiesCE">
		<damageDef>Hauts_PsychicBolt</damageDef>
		<damageAmountBase>13</damageAmountBase>
		<speed>178</speed>
		<secondaryDamage>
			<li>
				<def>Bomb_Secondary</def>
				<amount>2</amount>
			</li>
		</secondaryDamage>
		<armorPenetrationSharp>33</armorPenetrationSharp>
		<armorPenetrationBlunt>60</armorPenetrationBlunt>
	</projectile>
  </ThingDef>
  
  <ThingDef ParentName="BaseBulletCE" MayRequire="Ludeon.RimWorld.Royalty">
    <defName>HVT_ZizBlastProjectile_CE</defName>
    <label>pulverization blast projectile</label>
    <graphicData>
      <texPath>Things/Ethereal/Tornado</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <thingClass>CombatExtended.ProjectileCE_Explosive</thingClass>
	<projectile Class="CombatExtended.ProjectilePropertiesCE">
		<damageDef>Hauts_SkipFrag</damageDef>
		<speed>80</speed>
		<damageAmountBase>9</damageAmountBase>
		<dropsCasings>false</dropsCasings>
		<explosionRadius>1.9</explosionRadius>
		<explosionDelay>10</explosionDelay>
		<armorPenetrationSharp>999</armorPenetrationSharp>
		<armorPenetrationBlunt>999</armorPenetrationBlunt>
	</projectile>
  </ThingDef>
</Defs>